---
interface arr {
  name: string;
  description: string;
}
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header.astro";
import Card from "../components/Card.astro";
import Services from "../components/Services.astro";
import Footer from "../components/Footer.astro";
import Fullscreen from "../components/Fullscreen.astro";
const protoArr: arr[] = [
  { name: "Concerts", description: "My concerts." },
  { name: "Nature", description: "My photos of nature." },
  { name: "Concerts", description: "My concerts." },
  { name: "Nature", description: "My photos of nature." },
];
---

<Layout title="The Portfolio">
  <main>
    <Fullscreen />
    <Header />
    {
      protoArr.map((obj) => {
        return <Card name={obj.name} description={obj.description} />;
      })
    }
    <Services />
    <Footer />
  </main>
</Layout>

<style>
  main {
    position: relative;
  }
</style>

<script src="../script.ts"></script>
